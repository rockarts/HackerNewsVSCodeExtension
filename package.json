{
  "name": "hackernews",
  "displayName": "HackerNews",
  "description": "A HackerNews reader for VSCode",
  "version": "0.0.1",
  "publisher": "StevenRockarts",
  "engines": {
    "vscode": "^0.10.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:hackernews.topstories"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "hackernews.topstories",
        "title": "HackerNews: Display the top stories from HackerNews"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "moment": "^2.12.0"
  }
}
